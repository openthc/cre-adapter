{
	"name": "openthc/cre-adapter",
	"description": "OpenTHC Cannabis Reporting Engine API Adapters",
	"license": "MIT",
	"autoload": {
		"exclude-from-classmap": [
			"vendor/erusev/parsedown-extra/test"
		],
		"files": [
			"lib/CRE.php"
		],
		"psr-4": {
			"OpenTHC\\CRE\\": "./lib",
			"OpenTHC\\CRE\\Test\\": "./test"
		}
	},
	"require": {
		"php": ">=7.3.0",
		"ext-curl": "*",
		"ext-date": "*",
		"ext-json": "*",
		"ext-sockets": "*"
	},
	"require-dev": {
		"openthc/common": "dev-master",
		"phpstan/phpstan": "1.7.8",
		"phpunit/phpunit": "9.5.20"
	}
}
